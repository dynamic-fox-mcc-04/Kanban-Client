          email : '',\r\n                password : '',\r\n            },\r\n            isError : false,\r\n            errors : []\r\n        }\r\n    },\r\n    methods : {\r\n        isLogin(){\r\n            this.$emit('Status', false) \r\n        },\r\n\r\n        login(){\r\n            axios({\r\n                method : 'POST',\r\n                url : 'http://localhost:3000/login',\r\n                data : {\r\n                    email : this.data.email,\r\n                    password : this.data.password\r\n                }\r\n            })\r\n            .then(result => {\r\n                \r\n                this.isError = false\r\n                localStorage.setItem('access_token', result.data.access_token)\r\n                this.$emit('loginStatus', true)\r\n                clearForm();\r\n               \r\n            })\r\n            .catch(err => {\r\n\r\n                if ( err.response){\r\n                    this.isError = true\r\n                    this.errors = err.response.data.errors  \r\n                } \r\n            })\r\n        },\r\n\r\n        clearForm(){\r\n            this.data.email = '',\r\n            this.data.password = ''\r\n        },\r\n        \r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"023e31f2c4d00a532314c8856b85e976","cacheData":{"env":{}}}