-radius: 2px;\r\n  \r\n    box-shadow: 5px 0px 10px 0px rgb(177, 177, 177);\r\n}\r\n\r\n\r\n\r\nh2 {\r\n    color: $bgColor;\r\n    font-size: 24px;\r\n}\r\n\r\n.left {\r\n   margin: 10px;\r\n    \r\n}\r\n\r\n.right {\r\n    background-color:$bgColor;\r\n    width: 200px;\r\n    flex: 1;\r\n    \r\n}\r\n\r\n.right h1 {\r\n    color: white;\r\n    font-family: $lobster;\r\n    text-align: center;\r\n    font-size: 64px;\r\n    margin-top: 40%;\r\n    opacity: 90%;\r\n}\r\n\r\n.left form input[type=text]:focus {\r\n    border-bottom: 1px solid $bgColor;\r\n    box-shadow: 0 1px 0 0 #000;\r\n  }\r\n\r\np {\r\n    font-size: 12px;\r\n   color: grey;\r\n}\r\n\r\n\r\n//dashboard\r\n.container {\r\n    margin: 0;\r\n    padding: 0;\r\n    height: 100vh;\r\n    width: 100vw;\r\n    display:flex;\r\n    flex-direction:column;\r\n    background-color:$bgColor;\r\n    // @include bacgroundImage('/img/bg.jpg');\r\n}\r\n\r\n.navbar {\r\n    margin: 0;\r\n    padding: 0;\r\n    display: flex;\r\n    flex-direction: row;\r\n    background-color: $navColor;\r\n    height:60px;\r\n    opacity: 32%;\r\n    justify-content: space-between;\r\n\r\n} \r\n\r\n.kanban-title h4 {\r\n    color: white;\r\n    flex: 1;\r\n    margin: 5px;\r\n    font-family: $lobster;\r\n}\r\n\r\n.navbar #logout-btn {\r\n    opacity: 100%;\r\n    color: white;\r\n    border: none;\r\n    margin: 5px;\r\n    background-color:rgb(201, 201, 201);\r\n    border-radius: 2px;\r\n    width: 64px; \r\n   \r\n}\r\n\r\n.navbar #logout-btn:hover {\r\n    cursor: pointer; \r\n    // opacity: 50%;\r\n}\r\n\r\n\r\n.content {\r\n    height: 100vh;\r\n    display:flex;\r\n    justify-content: flex-start;\r\n    align-items: flex-start;\r\n    overflow-x: auto;\r\n   \r\n}\r\n\r\n.card-list, .card-list-add {\r\n    display: flex;\r\n    flex-direction: column; \r\n    background-color: $cardColor;\r\n    margin: 10px;\r\n    padding: 2px;\r\n    width: 240px;\r\n    border-radius: 3px;\r\n    min-width: 250px;\r\n}\r\n\r\n.card-list-add {\r\n    width: 200px;\r\n}\r\n\r\n.card-list-add div input {\r\n    margin: auto;\r\n}\r\n\r\n.card-title h3 {\r\n    font-size: 14px;\r\n    padding: 5px;\r\n    margin-top: 10px;\r\n \r\n}\r\n\r\n.card-title {\r\n    height: 40px;\r\n}\r\n\r\n.card-content-box {\r\n    display: flex;\r\n    flex-direction: column;\r\n    min-height: auto;\r\n    max-height: 200px;\r\n}\r\n\r\n.card-content {\r\n    background-color: white;\r\n    margin: 4px;\r\n    border-radius: 2px;\r\n    padding: 5px;\r\n    box-shadow: 1px 0px 2px 0px grey;\r\n    font-size: 14px;\r\n    display: flex;\r\n    flex-direction: column;   \r\n    justify-content: right;\r\n    height:auto;\r\n    width: auto; \r\n \r\n}\r\n\r\n.card-content button {\r\n    border: none;\r\n    height: 30px;\r\n    margin-left: 150px;\r\n}\r\n\r\n.card-content p {\r\n    margin: 0;\r\n    color: black;\r\n}\r\n\r\n.card-content button:hover {\r\n    cursor: pointer;\r\n}\r\n\r\n.card-content .options {\r\n    text-align: right;\r\n    margin: 0;\r\n}\r\n\r\n.card-content .options i:hover {\r\n    cursor: pointer;\r\n    display: block;\r\n}\r\n\r\n\r\n\r\n.card-btn button {\r\n    height:32px;\r\n    width: 100%;\r\n    text-align: left;   \r\n    background-color: $cardColor; \r\n    border: 0;\r\n    padding: 5px;\r\n    border-radius: 3px;\r\n   \r\n}\r\n\r\n.card-btn button:hover {\r\n   background-color: $cardColorHover;\r\n   cursor: pointer;\r\n  \r\n}\r\n\r\n.card-btn {\r\n    display: flex;\r\n    flex-direction: row;\r\n    padding: 5px;\r\n}\r\n\r\n.card-btn #edit {\r\n    width: auto;\r\n}\r\n\r\n.card-btn #delete {\r\n    flex: 1;\r\n}\r\n\r\n//dropdown\r\n.card-content:hover {\r\n    background-color: rgb(245, 245, 245);\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n// .card-content:hover ul li ul.dropdown {\r\n//     display: none;\r\n// }\r\n\r\nul{\r\n    margin: 0;\r\n    padding: 0;\r\n    list-style: none;\r\n    background:none;\r\n    text-align: right;\r\n    height: auto;\r\n    \r\n}\r\nul li{\r\n    display: inline-block;\r\n    position: relative;\r\n    line-height: 32px;\r\n}\r\nul li a{\r\n    display: block;\r\n    padding: 0px 10px;\r\n    color: gray;\r\n    text-decoration: none;\r\n    text-align: left;\r\n\r\n\r\n}\r\nul li a:hover{\r\n    color: #fff;\r\n    background: $bgColor;\r\n}\r\n\r\nul li ul.dropdown{\r\n     /* Set width of the dropdown */\r\n    width: 200;\r\n    background: #f2f2f2;\r\n    position: absolute;\r\n    z-index: 999;\r\n    left: 0;\r\n    display: none;\r\n  \r\n    \r\n}\r\n\r\n\r\nul li:hover ul.dropdown{\r\n    display: block;\t/* Display the dropdown */\r\n}\r\nul li ul.dropdown li{\r\n    display: block;\r\n    width: 160px;\r\n}\r\n\r\n\r\n.move {\r\n    display: flex;\r\n    flex-direction: row;\r\n\r\n}\r\n\r\n.move select {\r\n    flex: 1;\r\n    width: auto;\r\n}\r\n\r\n.move button {\r\n    margin: 2px;\r\n    height: auto;\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"2bbd9e565a0d853f8d19582882af5351","cacheData":{"env":{}}}